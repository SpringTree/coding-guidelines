{
  "extends": "tslint-config-airbnb",
  "rules": {
    "no-constant-condition": true,
    "max-line-length": [ true, 160 ],
    "space-within-parens": [ true, 1 ],
    "space-in-parens": [true, "always"],
    "no-console": false
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
