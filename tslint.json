{
  "extends": "tslint-config-airbnb",
  "linterOptions": {
      "exclude": [
          "node_modules/**"
      ]
  },
  "rules": {
    "max-line-length": [ true, 160 ],
    "no-console": false,
    "variable-name": { "options": [ "allow-leading-underscore" ] }
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
